# Make file for GMRFilm program using GNU $(FC) $(FFLAGS) Fortran Compiler
# Invoke with "make -f gmrmake.txt" .

# old compiler g77 used: FC = g77
FC = gfortran
FFLAGS = -c -g -Wall

.o: $(FC) $(FFLAGS) $*.f


OBJECTS = GMRFILM.o ABSCOEFF.o ABSIONRT.o ARFC.o BASTBETA.o CHARFDTA.o CHARFLR.o \
	  CHIOV.o CK.o COMPSTD.o CONTFLR.o DIFFEREN.o DLINES.o EDGE.o \
	  EFFYLD.o EI.o EIDP.o ELEMENTS.o ERFC.o FIXLAYER.o FLUORPRN.o \
	  FTEST.o GETCFG.o GETFNAME.o GETKRAT.o GETMODEL.o GETTOA.o GETVOLT.o \
	  HELP.o HELPFILE.o INIT0.o INIT1.o INIT2.o INIT3.o INIT4.o \
	  INIT5.o INTEGRAL.o LAYR.o LAYRDENS.o LAYRELEM.o LISTEL.o LOOKUP.o \
	  MAC.o MACSTD.o MU.o MULAYELM.o NEDGE.o NEWLINE.o NEWVOLT.o \
	  NEXTCALC.o OMEGA.o PAP.o PAPEI.o PAPEX.o PAPEXEI.o PAPFLUOR.o \
	  PAPINT.o PAPLIMTS.o PAPWT.o PHIRZEQ.o PPINT1.o PPINT2.o \
	  PRINTOUT.o QE0.o RB2A.o RJUMP.o SCALE.o STANDARD.o STOICH0.o \
	  STOICH1.o STRTTHCK.o TITL.o TRIPINT.o TRNSPROB.o WTFRACT.o \
	  XRAY.o ZNL.o


gmr: $(OBJECTS)
	$(FC)  $(OBJECTS) -o gmr

# $(OBJECTS): defs.h ftypes.h defines.h machdefs.h sysdep.h

